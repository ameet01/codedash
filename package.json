    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "^2.6.9",
    "express": "^4.18.1",
    "express-session": "^1.15.6",
    "mongod": "^2.0.0",
    "mongodb": "^4.9.1",
    "mongoose": "^6.6.0",
    "morgan": "^1.9.0",
    "nodemon": "^2.0.19",
    "passport": "^0.6.0",
